<% include ./partials/_header %>
<body style="font-size:20px; padding:50px;">
  <% if (!user) { -%>
    <div>Please register or login</div>
  <% } -%>
  <h3>My URLs:</h3>
  <% if (urls) { %>
    <% for (let key in urls) { -%>
      <li>
        <%= key  %> => <%= urls[key] %>
        <a href='/urls/<%= key %>'>Edit</a>
        <form method="POST" action="/urls/<%= key %>/delete">
          <button type="submit" class="btn btn-primary">Delete</button>
        </form>
      </li>
    <% } -%>
  <% } %>

</body>
</html>